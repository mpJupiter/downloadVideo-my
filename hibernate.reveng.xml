<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="t_customer" catalog="downloadvideo"
		class="com.download.model.Customer">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_video" catalog="downloadvideo"
		class="com.download.model.Video">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="t_videotype" catalog="downloadvideo"
	class="com.download.model.Videotype">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>